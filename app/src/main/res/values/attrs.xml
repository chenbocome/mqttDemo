<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StarBar">
        <!--星星间距-->
        <attr name="starDistance" format="dimension"/>
        <!--尺寸值-->
        <attr name="starSize" format="dimension"/>
        <!--星星总数-->
        <attr name="starCount" format="integer" />
        <!--当前进度float值-->
        <attr name="starStep" format="float" />
        <!--空白的星星资源文件值-->
        <attr name="starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFull" format="reference" />
        <!--是否只显示-->
        <attr name="onlyShow" format="boolean"/>
    </declare-styleable>

</resources>